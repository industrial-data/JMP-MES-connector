colListGroup << Append( colListData << GetSelected );
);


OKbuttonPress=Function({this},
	
	// asigns  current data table to dt
	dt = CurrentDataTable();
	
	// gets current user selection
	columnNameList = colListGroup << GetItems;//List of columns in this form: {"column_1","column_2",...,"column_3"}
	
	// gets naming specified by user
	myGroupName = GroupName << GetText;
	
	myGroup = dt << GroupColumns( myGroupName, columnNameList );
	
	print(myGroupName || " created");
	
	
	// saves options for recall (if needed)
	recallRoles; 
	
	colListData << reset;
	colListData << NameContains (" ");
	
	//colListData << delete;
	//list2 << prepend(colListData = Filter Col Selector());
);


ResetButtonPress=Function({this},
