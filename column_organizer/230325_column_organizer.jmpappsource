PasteButtonPress=Function({this},
	copiedColumns = GetClipboard();
	copiedColumnList = Words(copiedColumns,"\!t\!r\!n");
	colListGroup<<Append(copiedColumnList);
);


RecallButtonPress=Function({this},
	clearRoles;
	/* Restore any previous settings from the global variables */
	Try(
		colListGroup << Append( ::colListGroupRecall );
		GroupName << SetText( ::GroupNameRecall );
	);
);

