RenameButtonPress= Function( {this},
	print("Renaming columns");

	dt = CurrentDataTable();
	
	selectedElements = colListGroup << Get Items;
	
	show(N ItemS(selectedElements));
	
	Type_NameRadioBox = NameRadioBox  << Get Selected;
	
	show(Type_NameRadioBox);
	
	print("Iterating through column names:");
		For(i = 1, i <= N Items(selectedElements), i++,
			col_name = selectedElements[i];
			show(col_name);
			
			new_name = selectedElements[i]; // in case something fails

			print("Saving original column name as metadata");
			
			orig_col_name = Column(dt, col_name) << Get Property("original_colname");
			
			if(is Empty(orig_col_name),
			// Original property not exisitng
			Column(dt, col_name) << Set Property("original_colname", col_name);,
			//Restablishing original name
				new_name = orig_col_name;
				show(new_name);
			);
						
			
			if( Type_NameRadioBox == "Tagname",
				Try(new_name = Column(dt, col_name) << Get Property("tagname");
					show(new_name);
				);
			);
			
			if( Type_NameRadioBox == "Tag (description) [units]",
				Try(new_name = Column(dt, col_name) << Get Property("addin_colname");
					show(new_name););
			);
			
			if( Type_NameRadioBox == "Formula",	
				print("Trying to get formula from column");
				new_name = Char(Column(dt, col_name) << Get Formula);
				if(new_name != Char(Empty()),
					substitute into(new_name, expr(":"), expr(""));
					substitute into(new_name, expr("\!"n)"), expr("\!")"));
					show(new_name);,
					
					print("No formula was found");
					new_name = col_name;
					);
				);
						
			if( Type_NameRadioBox == "original_colname",	
				Try( new_name = Column(dt, col_name) << Get Property("original_colname");
				show(new_name););
			);
			
			print("Renaming column");
			Column( dt, col_name ) << Set Name(new_name);

		);
	);


