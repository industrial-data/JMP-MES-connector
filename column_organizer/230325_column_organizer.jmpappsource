// Expression to store the current settings in global variable
recallRoles = Expr(
	::colListGroupRecall = colListGroup << GetItems;
	::GroupNameRecall = GroupName << GetText();
);

// Clear all current settings
clearRoles = Expr(
	colListGroup << RemoveAll;
	GroupName << SetText("Group name");
);

UngroupButtonPress=Function({this},
	// This function is called when the button is pressed
	// gets current user selection
	//List of columns in this form: {"column_1","column_2",...,"column_3"}
	selectedElements = colListData << GetSelected;

	dt << UngroupColumns(selectedElements);
	colListData << Reset;
	colListData << NameContains (" ");
);


ToFirstButtonPress=Function({this},
	// This function is called when the button is pressed
	selectedElements = colListData << GetSelected;
	
	// Works in JMP 15
	//dt << move selected columns( selectedElements, To First);

	
	// bug in JMP 14:
	// https://community.jmp.com/t5/Discussions/Move-Selected-Columns-with-list-parameter/td-p/108064 
	Eval( Eval Expr(dt << Move Selected Columns( Expr(selectedElements), To First)) ); 
	colListData << Reset; //refresh selector
	colListData << NameContains (" ");
);


ToLastButtonPress=Function({this},
	// This function is called when the button is pressed
	selectedElements = colListData << GetSelected;
	
	// Works in JMP 15
	//dt << move selected columns( selectedElements, To Last);

	
	// bug in JMP 14:
	// https://community.jmp.com/t5/Discussions/Move-Selected-Columns-with-list-parameter/td-p/108064 
	Eval( Eval Expr(dt << Move Selected Columns( Expr(selectedElements), To Last)) ); 
	colListData << Reset; //refresh selector
	colListData << NameContains (" ");
);

MoveButtonPress=Function({this},
	selectedElements = colListData << GetSelectedColumns;
	dt << MoveSelectedColumns(selectedElements, after(:name)); 
	Wait(0);
	colListData << Reset; //refresh selector
	colListData << NameContains (" ");
);

