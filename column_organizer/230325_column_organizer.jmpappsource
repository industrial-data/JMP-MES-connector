DuplicatePress = Function( {this},
	selectedElements = colListGroup << GetSelected;
	
	if(N Items(selectedElements) ==0, Throw());
	
	//colListGroup << RemoveAll;
	
	fDupCol = function({colname, new = "", dt = Current Data Table() },
		col = Column(dt, colname);
	
		if(IsMissing(new), new = colname||" - Copy");
	
		col_script = char(col << Get Script);
		col_script = substitute(col_script, "New Column", "dt << New Column");
		col_script = substitute(col_script, colname, new);
		eval(parse(col_script));
		
		colListGroup << Append( new );
	);
	
	For( i = 1, i <= N Items(selectedElements), i++,
		selectedElement = selectedElements[i];
		print("Duplicating column", selectedElement);
		fDupCol(selectedElement);
		);
);	
	
